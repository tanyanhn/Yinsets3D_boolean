    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/YSmeet.exe",
            // "program": "${workspaceRoot}/build/test/reFacBoundarytest.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "windows": {
                "miDebuggerPath": "C:\\Program Files (x86)\\mingw-w64\\i686-8.1.0-posix-dwarf-rt_v6-rev0\\mingw32\\bin\\gdb.exe"
            },
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
